{
  "strict" : true,
  "curly" : true,
  "eqeqeq" : true,
  "forin" : true,
  "freeze" : true,
  "immed" : true,
  "indent" : 2,
  "latedef" : true,
  "newcap": true,
  "noarg" : true,
  "noempty" : true,
  "quotmark": "single",
  "undef" : true,
  "unused" :true,
  "trailing" : true,
  "maxparams" : 4,
  "maxlen" : 200,
  "smarttabs" : true,
  "globals" : {
    "$" : true,
    "console" : true,
    "document" : true,
    "jQuery" : true,
    "Modernizr": true,
    "module" : true,
    "require" : true,
    "requirejs" : true,
    "window": true,
    "history": true,
    "alert": true,
    "lang": true,
    "location": true,
    "setTimeout": true,
    "clearTimeout": true
  }
}
